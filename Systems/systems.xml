<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <electrical>
    <on                       type="bool">false</on>
    <volts                    type="double">20</volts>
    <outputs>
      <instrument-lights-norm type="float">0.8</instrument-lights-norm>
      <audio-panel            type="double">0.0</audio-panel>
      <avionics-fan           type="double">0.0</avionics-fan>
      <KNS80                  type="string">28</KNS80>
      <adf                    type="double">28</adf>
      <nav                    type="double" n="0">0.0</nav>
      <nav                    type="double" n="1">0.0</nav>
      <comm                   type="double" n="0">28</comm>
      <comm                   type="double" n="1">28</comm>
      <dme                    type="double" n="0">0.0</dme>
      <dme                    type="double" n="1">0.0</dme>
      <hsi                    type="double">0.0</hsi>
      <transponder            type="double">0.0</transponder>
      <turn-coordinator       type="double">0.0</turn-coordinator>
    </outputs>
  </electrical>

  <filter>
    <type>gain</type>
    <gain>0.3048</gain>
    <input>/position/altitude-agl-ft</input>
    <reference>1.0</reference>
    <output>/position/altitude-agl-m</output>
  </filter>

</PropertyList>
