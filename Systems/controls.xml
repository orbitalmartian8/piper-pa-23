<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <lighting>
    <instrument-lights type="bool">false</instrument-lights>
    <nav-lights        type="bool">false</nav-lights>
    <taxi-lights       type="bool">false</taxi-lights>
    <beacon            type="bool">false</beacon>
    <strobe            type="bool">false</strobe>
    <landing-lights    type="bool">false</landing-lights>
    <luminosite-lights type="float">1</luminosite-lights>
    <instruments-norm  type="float">0.8</instruments-norm>
    <floods-lights     type="float">0.8</floods-lights>
    <elt               type="bool">false</elt>
  </lighting>

  <electric>
    <battery-switch    type="bool">false</battery-switch>
    <avionics-switch   type="bool">false</avionics-switch>
    <engines>
      <engine          n="0">
        <fuel-pump     type="bool">false</fuel-pump>
      </engine>
      <engine          n="1">
        <fuel-pump     type="bool">false</fuel-pump>
      </engine>
    </engines>
  </electric>

  <engines>
    <engine            n="0">
      <boost           type="double">0</boost>
      <magnetos        type="int">0</magnetos>
      <noze-flaps-norm type="float">1</noze-flaps-norm>
      <cowl-flaps-norm type="float">1</cowl-flaps-norm>
    </engine>
    <engine            n="1">
      <boost           type="double">0</boost>
      <magnetos        type="int">0</magnetos>
      <noze-flaps-norm type="float">1</noze-flaps-norm>
      <cowl-flaps-norm type="float">1</cowl-flaps-norm>
    </engine>
  </engines>

  <gear>
    <brake-parking     type="bool">true</brake-parking>
    <tailwheel-lock    type="bool">true</tailwheel-lock>
  </gear>

  <flight>
    <rudder-trim       type="double"> 0.000 </rudder-trim>
    <aileron-trim      type="double"> 0.008 </aileron-trim>
    <elevator-trim     type="double"> 0.060 </elevator-trim>
  </flight>

</PropertyList>
